<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
       default-autowire="byName">

    <bean id="arbitrateConfigImpl" class="com.alibaba.otter.manager.biz.common.arbitrate.ArbitrateConfigImpl"
          depends-on="zookeeperClient" lazy-init="false"/>

    <bean id="deadNodeListener" class="com.alibaba.otter.manager.biz.common.arbitrate.DeadNodeListener"
          depends-on="arbitrateConfigImpl" lazy-init="false">
    </bean>

</beans>